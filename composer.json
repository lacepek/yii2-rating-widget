{
    "name": "lacepek/yii2-rating-widget",
    "description": "Yii2 rating widget",
    "type": "yii2-extension",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Ladislav Cepek",
            "email": "la.cepek@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/lacepek/yii2-tcc-rating-widget/issues",
